    "editor.minimap.showSlider": "always",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.enableBell": true,
    "terminal.integrated.enableFileLinks": "on",
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.shellIntegration.enabled": true,
    "editor.cursorBlinking": "phase",
    "editor.matchBrackets": "always",
    "editor.guides.bracketPairs": "active",
    "editor.language.brackets": [
        [
            "[",
            "]"
        ],
        [
            "{",
            "}"
        ],
        [
            "(",
            ")"
        ]
    ],
    "editor.guides.highlightActiveIndentation": true,
    "editor.rulers": [
        80
    ],
    "editor.tokenColorCustomizations": {
        "functions": "#52DD7C",
        "variables": "#59DDF5",
        "numbers": "#FF85A7",
        "types": "#59DDF5",
        "strings": "#FFFF55",
        "comments": "#8681C9",
        "keywords": "#FFFFFF",
        "[MS-DOS Editor Theme]": {
            "textMateRules": [
                {
                    "scope": [
                        "graphics.QB64"
                    ],
                    "settings": {
                        "foreground": "#C26FDB"
                    }
                },
                {
                    "scope": [
                        "sound.QB64"
                    ],
                    "settings": {
                        "foreground": "#FFBB00"
                    }
                },
                {
                    "scope": [
                        "keyword.control.QB64"
                    ],
                    "settings": {
                        "foreground": "#95EBF1"
                    }
                },
                {
                    "scope": [
                        "debug.QB64"
                    ],
                    "settings": {
                        "foreground": "#FF85A7"
                    }
                },
                {
                    "scope": "entity.name.tag",
                    "settings": {
                        "foreground": "#FFFFFF"
                    }
                },
                {
                    "scope": "userfunctions.QB64",
                    "settings": {
                        "foreground": "#52DD7C"
                    }
                },
                {
                    "scope": "entity.name.tag.css",
                    "settings": {
                        "foreground": "#55FF55"
                    }
                },
                {
                    "scope": "entity.other.attribute-name",
                    "settings": {
                        "foreground": "#55FFFFFF"
                    }
                },
                {
                    "scope": "string.quoted.double",
                    "settings": {
                        "foreground": "#FFFF55"
                    }
                },
                {
                    "scope": "comment.line.double-slash",
                    "settings": {
                        "foreground": "#55FFFF"
                    }
                },
                {
                    "scope": "variable.parameter",
                    "settings": {
                        "foreground": "#FFFF55"
                    }
                },
                {
                    "scope": "entity.other.attribute-name",
                    "settings": {
                        "foreground": "#55FF55"
                    }
                },
                {
                    "scope": "variable.other.readwrite",
                    "settings": {
                        "foreground": "#59DDF5"
                    }
                },
                {
                    "scope": "support.constant.property-value",
                    "settings": {
                        "foreground": "#93C4EB"
                    }
                },
                {
                    "scope": "support.constant.value",
                    "settings": {
                        "foreground": "#93C4EB"
                    }
                },
                {
                    "scope": "support.constant.font-name.css",
                    "settings": {
                        "foreground": "#93C4EB"
                    }
                },
                {
                    "scope": "support.constant.color.w3c-extended-color-name.css",
                    "settings": {
                        "foreground": "#93C4EB"
                    }
                },
                {
                    "scope": "support.constant.color.w3c-standard-color-name.css",
                    "settings": {
                        "foreground": "#93C4EB"
                    }
                },
                {
                    "scope": "variable.other.QB64",
                    "settings": {
                        "foreground": "#52DD7C",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "keyword.other.unit",
                        "meta.link.inline",
                        "storage.modifier.async.js"
                    ],
                    "settings": {
                        "foreground": "#FF85A7"
                    }
                },
                {
                    "scope": [
                        "markup.heading.markdown",
                        "markup.heading.setext"
                    ],
                    "settings": {
                        "foreground": "#55FF55"
                    }
                },
                {
                    "scope": [
                        "markup.fenced_code.block",
                        "punctuation.definition.list",
                        "markup.list"
                    ],
                    "settings": {
                        "foreground": "#FFFF55"
                    }
                }
            ]
        }
    },
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.guides.highlightActiveBracketPair": true,
    "editor.quickSuggestionsDelay": 3000,
    "debug.openDebug": "openOnDebugBreak",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "autoalign.moveableItemsAdditional": [
        "AS"
    ],
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "markdown.preview.openMarkdownLinks": "inPreview",
    "markdown.links.openLocation": "beside",
    "markdown.preview.linkify": true,
    "githubPullRequests.pullBranch": "never",
    "editor.quickSuggestions": {
        "comments": "inline"
    },
    "editor.suggestOnTriggerCharacters": false,
    "editor.tabCompletion": "on",
    "githubPullRequests.ignoredPullRequestBranches": [
        "main"
    ],

    "autoalign.minSeparationLeft": 1,
    "autoalign.columnWidth": 1,
    "git.openRepositoryInParentFolders": "never",
    "editor.accessibilitySupport": "off",
    "workbench.editorAssociations": {
        "*.md": "vscode.markdown.preview.editor"
    },
    "workbench.list.multiSelectModifier": "ctrlCmd",
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.editor.enablePreview": true,
    "editor.renderWhitespace": "none",
    "debug.allowBreakpointsEverywhere": true,
    "editor.stickyScroll.enabled": true,
    "files.associations": {
        "*.TXT": "plaintext"
    },
    "markdown.preview.fontSize": 18,
    "terminal.integrated.fontSize": 13,
    "forbeslindesay-taskrunner.auto-expand-limit": 20,
    "[QB64]": {
        "editor.stickyScroll.enabled": true
    },
    "workbench.iconTheme": "file-icons"
}
